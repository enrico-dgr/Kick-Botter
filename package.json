{
  "name": "kickbotter",
  "version": "1.1.4",
  "main": "./index.js",
  "description": "Automated socials' actions",
  "author": "enrico-dgr",
  "repository": {
    "url": "https://github.com/enrico-dgr/Kick-Botter"
  },
  "scripts": {
    "runFile": "ts-node ",
    "install-build-deps": "cd ./KickBotter && npm install",
    "-": "echo '-------------------------- This is just a comment command' ",
    "prebuild:main:dev": "ts-node ./Scripts/dev.ts",
    "build:main:dev": "tsc -p tsconfig.build.main.json",
    "build:renderer:dev": "npx webpack --config webpack.config.js",
    "--": "echo '-------------------------- This is just a comment command' ",
    "build:main:prod": "tsc -p tsconfig.build.main.prod.json",
    "build:renderer:prod": "npx webpack --config webpack.config.prod.js",
    "prebuild:prod": "ts-node ./Scripts/prod.ts && npm run install-build-deps",
    "build:prod": "npm run build:main:prod && npm run build:renderer:prod",
    "---": "echo '-------------------------- This is just a comment command' ",
    "prelocal:build:prod": "ts-node ./Scripts/dev.ts",
    "local:build:prod": "npm run build:main:prod && npm run build:renderer:prod",
    "----": "echo '-------------------------- This is just a comment command' ",
    "start": "cd ./KickBotter && electron-forge start",
    "package": "cd ./KickBotter && electron-forge package",
    "make": "cd ./KickBotter && electron-forge make",
    "publish": "cd ./KickBotter && electron-forge publish"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-dmg": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@electron-forge/publisher-github": "^6.0.0-beta.57",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.14.1",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.18",
    "@types/rimraf": "^3.0.1",
    "css-loader": "^5.2.6",
    "electron": "13.1.6",
    "eslint": "^7.29.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "fp-ts": "^2.10.5",
    "instauto": "^7.1.1",
    "io-ts": "^2.2.16",
    "launch-page": "^1.0.9"
  },
  "config": {
    "forge": "forge.config.js"
  }
}
