{
  "name": "kickbotter",
  "version": "1.0.1",
  "main": "./view/src/index.js",
  "description": "Automated socials' actions",
  "author": "enrico-dgr",
  "repository": {
    "url": "https://github.com/enrico-dgr/Kick-Botter"
  },
  "scripts": {
    "install:view": "cd view && npm install",
    "install:logic": "cd logic && npm install",
    "install:all": "npm run install:view && npm run install:logic && cd KickBotter npm install",
    "dev:build:main": "cd view && npm run build:main",
    "dev:build:renderer": "cd view && npm run build:renderer",
    "dev:start": "cd view && npm run start",
    "build:prod:view": "cd view && npm run build:prod",
    "build:prod": "npm run build:prod:view",
    "start": "cd ./KickBotter && electron-forge start",
    "package": "cd ./KickBotter && electron-forge package",
    "make": "cd ./KickBotter && electron-forge make",
    "publish": "cd ./KickBotter && electron-forge publish"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-dmg": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@electron-forge/publisher-github": "^6.0.0-beta.57",
    "electron": "13.1.6"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "fp-ts": "^2.10.5",
    "io-ts": "^2.2.16",
    "launch-page": "^1.0.9",
    "update-electron-app": "^2.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "kickbotter",
        "win32metadata": {
          "FileDescription": "Automated socials' actions",
          "InternalName": "kickbotter",
          "OriginalFilename": "kickbotter",
          "ProductName": "kickbotter"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Kickbotter",
            "exe": "KickBotter.exe",
            "authors": "Enrico Di Grazia",
            "description": "Some automated browser program."
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": "enrico-dgr",
              "homepage": "https://github.com/enrico-dgr/Kick-Botter",
              "description": "Some automated browser program.",
              "productDescription": "Some automated browser program."
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "platforms": [
            "darwin"
          ],
          "config": {
            "repository": {
              "owner": "enrico-dgr",
              "name": "Kick-Botter"
            }
          },
          "draft": true
        }
      ]
    }
  }
}
