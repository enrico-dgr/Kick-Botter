{
  "name": "kickbotter",
  "version": "1.0.2",
  "main": "./index.js",
  "description": "Automated socials' actions",
  "author": "enrico-dgr",
  "repository": {
    "url": "https://github.com/enrico-dgr/Kick-Botter"
  },
  "scripts": {
    "clean": "rimraf ./KickBotter",
    "-": "echo '-------------------------- This is just a comment command' ",
    "predev:build:main": "npm run clean && ts-node ./Scripts/dev.ts",
    "dev:build:main": "tsc -p tsconfig.build.json",
    "dev:build:renderer": "npx webpack --config webpack.config.js",
    "dev:start": "cd Electron && npm run start",
    "--": "echo '-------------------------- This is just a comment command' ",
    "build:main:prod": "tsc -p tsconfig.build-prod.json",
    "build:renderer:prod": "npx webpack --config webpack.config-prod.js",
    "prebuild:prod": "npm run clean  && ts-node ./Scripts/prod.ts",
    "build:prod": "npm run build:main:prod && npm run build:renderer:prod",
    "postbuild:prod": "cd ./KickBotter && npm install",
    "---": "echo '-------------------------- This is just a comment command' ",
    "start": "cd ./KickBotter && electron-forge start",
    "package": "cd ./KickBotter && electron-forge package",
    "make": "cd ./KickBotter && electron-forge make",
    "publish": "cd ./KickBotter && electron-forge publish"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-dmg": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@electron-forge/publisher-github": "^6.0.0-beta.57",
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.14.1",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "css-loader": "^5.2.6",
    "electron": "13.1.6",
    "eslint": "^7.29.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "fp-ts": "^2.10.5",
    "io-ts": "^2.2.16",
    "launch-page": "^1.0.9",
    "update-electron-app": "^2.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "kickbotter",
        "win32metadata": {
          "FileDescription": "Automated socials' actions",
          "InternalName": "kickbotter",
          "OriginalFilename": "kickbotter",
          "ProductName": "kickbotter"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Kickbotter",
            "exe": "KickBotter.exe",
            "authors": "Enrico Di Grazia",
            "description": "Some automated browser program."
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": "enrico-dgr",
              "homepage": "https://github.com/enrico-dgr/Kick-Botter",
              "description": "Some automated browser program.",
              "productDescription": "Some automated browser program."
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          }
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "platforms": [
            "darwin"
          ],
          "config": {
            "repository": {
              "owner": "enrico-dgr",
              "name": "Kick-Botter"
            }
          },
          "draft": true
        }
      ]
    }
  }
}
