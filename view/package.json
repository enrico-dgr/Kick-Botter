{
  "name": "view",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild:all": "rimraf ./build && mkdir build && cp ./src/index.html ./build",
    "build:all": "tsc -p tsconfig.build.json",
    "build": "tsc -p tsconfig.build.json",
    "start:electron": "electron ./build/view/src/index.js",
    "build:webpack": "npx webpack --config webpack.config.js",
    "start": "concurrently \"npm:start:electron\" \"npm:build:webpack\" ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:build": "tsc -b",
    "prebuild:prepackage": "rimraf ./KickBotter",
    "build:prepackage": "tsc -p tsconfig.build-prepackage.json",
    "postbuild:prepackage": "export NODE_ENV=packaging && npm run build:webpack && npm run completePackage",
    "completePackage": "cp ./src/index.html ./KickBotter",
    "installForge": "cd ./KickBotter && npm init -y && npm install --save-dev @electron-forge/cli && npx electron-forge import",
    "postcompletePackage": "npm run installForge",
    "prepackage": "npm run build:prepackage && npm run completePackage",
    "package": "cd ./KickBotter && npm run make",
    "tryPackage": "electron ./KickBotter/out/kickbotter-darwin-x64/kickbotter.app/Contents/Resources/app/view/src/index.js",
    "postpackage": "npm run tryPackage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.14.1",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "concurrently": "^6.2.0",
    "css-loader": "^5.2.6",
    "electron": "13.1.5",
    "eslint": "^7.29.0",
    "fp-ts": "^2.10.5",
    "jest": "^26.6.3",
    "launch-page": "^1.0.9",
    "prettier": "2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  }
}
