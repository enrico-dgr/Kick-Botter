{
  "name": "view",
  "description": "GUI",
  "author": "enrico-dgr",
  "scripts": {
    "build:main": "tsc -p tsconfig.build.json",
    "copyFilesOfView": "ts-node ./Scripts/copyFilesOfView.ts",
    "build:renderer": "npm run copyFilesOfView && npx webpack --config webpack.config.js",
    "clean": "rimraf ../KickBotter/view",
    "prebuild": "npm run clean",
    "build": "npm run build:main && npm run build:renderer",
    "start": "electron ../KickBotter",
    "dev:all": "concurrently \"npm:build\" \"npm:start\" ",
    "build:main:prod": "tsc -p tsconfig.build-prod.json",
    "build:renderer:prod": "npm run copyFilesOfView && npx webpack --config webpack.config-prod.js",
    "prebuild:prod": "npm run clean",
    "build:prod": "npm run build:main:prod && npm run build:renderer:prod"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.14.1",
    "@types/renderer": "^17.0.13",
    "@types/renderer-dom": "^17.0.8",
    "css-loader": "^5.2.6",
    "electron": "13.1.5",
    "eslint": "^7.29.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "renderer": "^17.0.2",
    "renderer-dom": "^17.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "io-ts": "^2.2.16"
  }
}
